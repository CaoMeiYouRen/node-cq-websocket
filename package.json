{
  "name": "cq-websocket",
  "version": "2.0.0",
  "description": "A Node SDK for developing QQ chatbots based on WebSocket, which is depending on CoolQ and CQHTTP API plugin. ",
  "main": "./src/index",
  "engines": {
    "cqhttp": "^4.10",
    "node": ">=8"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "tsc",
    "clean": "rimraf src/**/*.d.ts src/**/*.js .dist",
    "webpack": "webpack",
    "release": "semantic-release",
    "test": "env-cmd ./test.env nyc ava test/**/*.test.js",
    "test:dev": "ava test/**/*.test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "typedoc"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momocow/node-cq-websocket.git"
  },
  "keywords": [
    "coolq",
    "cqhttp",
    "coolq-http-api",
    "websocket"
  ],
  "author": "MomoCow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/momocow/node-cq-websocket/issues"
  },
  "homepage": "https://github.com/momocow/node-cq-websocket#readme",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/websocket": "0.0.40",
    "ava": "^1.4.1",
    "chalk": "^2.4.2",
    "commitizen": "^3.0.7",
    "coveralls": "^3.0.3",
    "env-cmd": "^8.0.2",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.1",
    "is-ci": "^2.0.0",
    "kaomojify-webpack-plugin": "^0.1.1",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.12",
    "semver": "^6.0.0",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "optionalDependencies": {
    "websocket": "^1.0.28",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v15.10.0"
  }
}
